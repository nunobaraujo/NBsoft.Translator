<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

	<system:String xml:space="preserve" x:Key="Yes">Yes</system:String>
	<system:String xml:space="preserve" x:Key="No">No</system:String>
	<system:String xml:space="preserve" x:Key="Ok">Ok</system:String>
	<system:String xml:space="preserve" x:Key="Cancel">Cancel</system:String>
</ResourceDictionary>